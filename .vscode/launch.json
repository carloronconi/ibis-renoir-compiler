    "version": "0.2.0",
    "configurations": [
        {
            "name": "Internal benchmark: debug",
            "type": "debugpy",
            "request": "launch",
            "module": "benchmark.internal.internal_benchmark",
            "console": "integratedTerminal",
            "args": ["--test_patterns", "test_nullable_filter_filter_select_select",
                    "--runs", "1", 
                    "--warmup", "0", 
                    "--table_origin", "cached", 
                    "--dir", "internal/banana", 
                    "--backends", "renoir"],
            "justMyCode": false,
            "subProcess": true
        },
        {
            "name": "Internal summary plot: debug",
            "type": "debugpy",
            "request": "launch",
            "module": "benchmark.internal.summary_plot",
            "console": "integratedTerminal",
            "args": ["log/internal/1M_all_sola1_6backs"],
            //"justMyCode": false,
            //"subProcess": true
        },
        {
            "name": "Hyperfine benchmark: flink on Q6",
            "type": "debugpy",
            "request": "launch",
            "module": "benchmark.hyperfine_benchmark",
            "console": "integratedTerminal",
            "args": ["test.test_nexmark.TestNexmark.test_nexmark_query_6", 
                    "--backend", "flink"],
            "justMyCode": false
        }
    ]
